<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Cryptobitfolio.UI.Exchange"
            xmlns:local1="clr-namespace:Cryptobitfolio.UI.Portfolio"
            xmlns:local2="clr-namespace:Cryptobitfolio.UI.Settings"
            xmlns:local3="clr-namespace:Cryptobitfolio.UI.Bot"
            x:Class="Cryptobitfolio.UI.Main.MainPage">
  <!--Pages can be added as references or inline-->
    <NavigationPage Title="Portfolio" Icon="portfolio.png">
        <x:Arguments>
            <local1:PortfolioPage />
        </x:Arguments>
    </NavigationPage>
    <local:ExchangePage />
    <local3:BotPage />
    <local2:SettingsPage />
</TabbedPage>