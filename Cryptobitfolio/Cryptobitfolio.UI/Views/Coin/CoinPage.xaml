<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cryptobitfolio.UI.Views.CoinPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Currency.SymbolLabel}" />
            <Label Text="{Binding Currency.NameLabel}" />
            <Label Text="{Binding QtyLabel}" />
            <Label Text="{Binding AvgBuyLabel}" />
            <Button Text="Cancel" Clicked="OnCancelClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>